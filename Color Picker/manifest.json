{
    "name": "Color picker",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version": 3,
    
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.jpg",
        "32": "icon.jpg",
        "48": "icon.jpg"
    }
}